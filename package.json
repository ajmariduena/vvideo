{
  "name": "vvideo",
  "version": "0.1.0",
  "description": "A simple Vue.js Video component",
  "homepage": "http://vvideo.ajmariduena.com/",
  "keyword": [
    "vue",
    "component",
    "video"
  ],
  "main": "dist/vvideo.js",
  "files": [
    "dist/",
    "umd/",
    "src/components/"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "poi --config .storybook/poi.config.js --storybook --open",
    "build:storybook": "poi build --config .storybook/poi.config.js --storybook",
    "build:component": "poi build src/components/Vvideo.vue --library",
    "build:component:umd": "poi build src/components/Vvideo.vue --library Vvideo --dist umd",
    "deploy": "surge .storybook/dist"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.0",
    "@storybook/addon-knobs": "^3.3.0",
    "@storybook/addon-options": "^3.3.0",
    "@storybook/vue": "^3.3.0",
    "html-loader": "^0.5.0",
    "markdown-loader": "^2.0.0",
    "marked": "^0.3.0",
    "poi": "^9.4.0",
    "poi-preset-resolve-alias": "^1.0.0",
    "poi-preset-storybook": "^0.1.0",
    "storybook-readme": "^3.1.0",
    "surge": "^0.19.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
